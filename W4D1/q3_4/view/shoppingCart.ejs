<%- include('fragments/head.ejs')%>
<body>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Price</th>
        <th>Quantity</th>
      </tr>
    </thead>
    <tbody>
      <% for(const [key, value] of Object.entries(products)){ %>
      <tr>
        <td><%= key %></td>
        <td><%= value["price"] %></td>
        <td><%= value["quantity"] %></td>
      </tr>
      <% } %>
    </tbody>
  </table>
</body>
<%- include('fragments/end.ejs')%>
